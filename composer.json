{
    "name": "lfbn/dynalist-php-client",
    "type": "library",
    "description": "A PHP Client for the Dynalist API",
    "keywords": [
        "api",
        "php",
        "dynalist"
    ],
    "homepage": "http://github.com/lfbn/dynalist-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Luís Nóbrega",
            "email": "luis.barros.nobrega@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "symfony/http-client": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "DynalistPhpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DynalistPhpClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "lint": "./vendor/bin/php-cs-fixer fix --show-progress=estimating"
    }
}
